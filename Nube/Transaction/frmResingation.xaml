<metro:MetroWindow 
    xmlns:metro="http://metro.mahapps.com/winfx/xaml/controls"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
                   GlowBrush="{DynamicResource AccentColorBrush}"
                   BorderThickness="1"
    x:Class="Nube.Transaction.frmResingation"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Nube.Transaction"
        mc:Ignorable="d"
        Title="NUBE Member Resignation" Tag="MemberResignation" Height="750" Width="1250" Icon="/Nube;component/Image/Logo.png" WindowState="Maximized">
	<metro:MetroWindow.Background>
		<ImageBrush ImageSource="/Nube;component/Image/Background.jpg"/>
	</metro:MetroWindow.Background>

	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Width="345"/>
			<ColumnDefinition Width="885"/>
			<ColumnDefinition Width="10*"/>
		</Grid.ColumnDefinitions>
		<Grid.RowDefinitions>
			<RowDefinition Height="50"/>
			<RowDefinition Height="570"/>
			<RowDefinition Height="10*"/>
		</Grid.RowDefinitions>
		<Grid Grid.Row="0" Grid.ColumnSpan="3">
			<materialDesign:Card Height="51" >
				<TextBox x:Name="txtHeading" FontWeight="Bold" TextAlignment="Center" BorderThickness="0" HorizontalAlignment="Left" Height="41" TextWrapping="Wrap" Text="MEMBER RESIGNATION" VerticalAlignment="Top" FontSize="18" Margin="540,0,0,0" />
			</materialDesign:Card>
		</Grid>
		<Grid Grid.RowSpan ="1" Grid.Column="0">
			<Rectangle Fill="White" Stroke="Black" StrokeThickness="0" HorizontalAlignment="Left" Width="334" Margin="15,90,-4,-655" />
			<Label x:Name="label" FontWeight="Bold" Content="Member Type" HorizontalAlignment="Left" VerticalAlignment="Top" Width="95" Margin="40,106,0,-82"/>
			<ComboBox IsEnabled="False" x:Name="cmbMemberType" Background="White" FontWeight="Bold" HorizontalAlignment="Left" VerticalAlignment="Top" Width="153" IsEditable="True" materialDesign:HintAssist.Hint="Member Type"  materialDesign:HintAssist.HintOpacity=".26" Margin="138,106,0,-83" Height="27" IsReadOnly="True" />
			<TextBox IsEnabled="False" x:Name="txtMemberNo" HorizontalAlignment="Left" Height="40" TextWrapping="Wrap" materialDesign:HintAssist.Hint="Membership" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalAlignment="Top" Width="251" FontWeight="Bold" Margin="40,140,0,-130" KeyDown="txtMemberNo_KeyDown" PreviewTextInput="txtMemberNo_PreviewTextInput"/>
			<ComboBox IsEnabled="False" x:Name="cmbMemberInit" FontWeight="Bold" Background="White" HorizontalAlignment="Left" VerticalAlignment="Top" Width="67" IsEditable="True" materialDesign:HintAssist.Hint="Title" materialDesign:HintAssist.HintOpacity=".26" Height="29" Margin="40,196,0,-175" IsReadOnly="True"/>
			<TextBox IsEnabled="False" x:Name="txtMemberName"  FontWeight="Bold" HorizontalAlignment="Left" Height="40"  TextWrapping="Wrap" materialDesign:HintAssist.Hint="Member Name" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalAlignment="Top" Width="181" Margin="110,185,0,-175" IsReadOnly="True" TextChanged="txtMemberName_TextChanged" />
			<Label x:Name="label_Copy" FontWeight="Bold" Content="DOB" HorizontalAlignment="Left" VerticalAlignment="Top" Width="55" Margin="40,243,0,-219"/>
			<Label x:Name="label_Copy1" FontWeight="Bold" Content="SEX" HorizontalAlignment="Left"  VerticalAlignment="Top" Width="55" Margin="40,286,0,-262"/>
			<Label x:Name="label_Copy3" FontWeight="Bold" Content="Date Joining" HorizontalAlignment="Left"  VerticalAlignment="Top" Width="85" Margin="35,468,0,-444"/>

			<DatePicker IsEnabled="False" x:Name="dtpDOB" FontWeight="Bold" HorizontalAlignment="Left" materialDesign:HintAssist.Hint="Date Of Birth" Style="{StaticResource MaterialDesignFloatingHintDatePicker}"  VerticalAlignment="Top" Width="106" FontSize="14" Margin="110,242,0,-222" SelectedDateChanged="dtpDOB_SelectedDateChanged" />
			<TextBox IsEnabled="False" x:Name="txtAge" FontWeight="Bold" HorizontalAlignment="Left" Height="40" TextWrapping="Wrap" materialDesign:HintAssist.Hint="Age" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalAlignment="Top" Width="56" Margin="235,232,0,-222" IsReadOnly="True"/>
			<ComboBox IsEnabled="False" x:Name="cmbGender" FontWeight="Bold" Background="White" HorizontalAlignment="Left" VerticalAlignment="Top" Width="106" IsEditable="True" materialDesign:HintAssist.Hint="Gender"  materialDesign:HintAssist.HintOpacity=".26" RenderTransformOrigin="1.318,0" Margin="110,282,0,-266" Height="34" IsReadOnly="True">
				<ComboBoxItem>Male</ComboBoxItem>
				<ComboBoxItem>Female</ComboBoxItem>
			</ComboBox>
			<CheckBox IsEnabled="False" x:Name="ckbRejoined" FontWeight="Bold" Content="Rejoined" HorizontalAlignment="Left" VerticalAlignment="Top" Width="93"  Margin="235,290,0,-266" Height="26"/>
			<ComboBox IsEnabled="False" x:Name="cmbRace" FontWeight="Bold" HorizontalAlignment="Left" Background="White" VerticalAlignment="Top" Width="106" IsEditable="True" materialDesign:HintAssist.Hint="Race"  materialDesign:HintAssist.HintOpacity=".26" Margin="111,324,0,-306" Height="32" IsReadOnly="True"/>
			<TextBox IsEnabled="False" x:Name="txtOldIC" FontWeight="Bold" HorizontalAlignment="Left" Height="40" TextWrapping="Wrap" materialDesign:HintAssist.Hint="NRIC-O" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalAlignment="Top" Width="177" Margin="40,363,0,-353" IsReadOnly="True"/>
			<TextBox IsEnabled="False" x:Name="txtNewIC" FontWeight="Bold" HorizontalAlignment="Left" Height="40"  TextWrapping="Wrap" materialDesign:HintAssist.Hint="NRIC-N" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalAlignment="Top" Width="177" Margin="40,409,0,-399" IsReadOnly="True"/>
			<DatePicker IsEnabled="False" x:Name="dtpDOJ" FontWeight="Bold" HorizontalAlignment="Left" materialDesign:HintAssist.Hint="Date Of Joining" Style="{StaticResource MaterialDesignFloatingHintDatePicker}" VerticalAlignment="Top" Width="105" FontSize="14" Margin="120,468,0,-448" />
			<Label x:Name="label_Copy2" FontWeight="Bold" Content="Race" HorizontalAlignment="Left" VerticalAlignment="Top" Width="55" Margin="40,327,0,-303"/>
			<TextBox IsEnabled="False" x:Name="txtRemarks"  FontWeight="Bold" HorizontalAlignment="Left" Height="50"  TextWrapping="Wrap" AcceptsReturn="True" materialDesign:HintAssist.Hint="Remarks" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalAlignment="Top" Width="250" Margin="40,520,0,-448" />
			<Button x:Name="btnSearch" Content="Select" HorizontalAlignment="Left" VerticalAlignment="Top" Width="87" Margin="138,585,0,-563" Height="28" Click="btnSearch_Click"/>
			<Button x:Name="btnHome" Content="Home" HorizontalAlignment="Left" VerticalAlignment="Top" Width="93" Margin="235,585,0,-563" Height="28" Click="btnHome_Click"/>
		</Grid>
		<Grid Grid.Column="1" Grid.Row="1">
			<TabControl x:Name="tabControl" HorizontalAlignment="Left" Height="560" VerticalAlignment="Top" Width="875" FontSize="14" Background="Transparent" Margin="10,10,0,0" >
				<TabControl.Resources>
					<Style TargetType="TabItem">
						<Setter Property="Template">
							<Setter.Value>
								<ControlTemplate TargetType="TabItem">
									<Grid Name="Panel">
										<ContentPresenter x:Name="ContentSite"
                                        VerticalAlignment="Center"
                                        HorizontalAlignment="Center"
                                        ContentSource="Header"
                                        Margin="10,2"/>
									</Grid>
									<ControlTemplate.Triggers>
										<Trigger Property="IsSelected" Value="True">
											<Setter TargetName="Panel" Property="Background" Value="LightSkyBlue" />
										</Trigger>
										<Trigger Property="IsSelected" Value="False">
											<Setter TargetName="Panel" Property="Background" Value="white" />
										</Trigger>
									</ControlTemplate.Triggers>
								</ControlTemplate>
							</Setter.Value>
						</Setter>
					</Style>
				</TabControl.Resources>
				<TabItem Header="IRC Confirmation" Name="tbiIRC" FontSize="14" >
					<ScrollViewer>
						<Grid Margin="0 0 0 0" Background="White">
							<Grid.RowDefinitions>
								<RowDefinition Height="40"/>
								<RowDefinition Height="250"/>
								<RowDefinition Height="20"/>
								<RowDefinition Height="330"/>
								<RowDefinition Height="250"/>
								<RowDefinition Height="50"/>
							</Grid.RowDefinitions>
							<Label Grid.Row="0" FontWeight="Bold" FontSize="22">IRC CONFIRMATION OF BENEVOLENT FUND APPLICATION</Label>
							<Grid Grid.Row="1">
								<Grid.RowDefinitions>
									<RowDefinition Height="*"/>
									<RowDefinition Height="*"/>
									<RowDefinition Height="*"/>
									<RowDefinition Height="*"/>
									<RowDefinition Height="*"/>
									<RowDefinition Height="*"/>
								</Grid.RowDefinitions>
								<Grid.ColumnDefinitions>
									<ColumnDefinition Width="150"/>
									<ColumnDefinition Width="30"/>
									<ColumnDefinition Width="*"/>
								</Grid.ColumnDefinitions>
								<Label Grid.Row="0" VerticalAlignment="Bottom" Grid.Column="0">Membership No</Label>
								<Label Grid.Row="1" VerticalAlignment="Bottom" Grid.Column="0">IRC Name in Full</Label>
								<Label Grid.Row="2" VerticalAlignment="Bottom" Grid.Column="0">IRC Position</Label>
								<Label Grid.Row="3" VerticalAlignment="Bottom" Grid.Column="0">Bank</Label>
								<Label Grid.Row="4" VerticalAlignment="Bottom" Grid.Column="0">Bank Address</Label>
								<Label Grid.Row="5" VerticalAlignment="Bottom" Grid.Column="0">Telephone/Fax No</Label>

								<Label Grid.Row="0" VerticalAlignment="Bottom" Grid.Column="1">:</Label>
								<Label Grid.Row="1" VerticalAlignment="Bottom" Grid.Column="1">:</Label>
								<Label Grid.Row="2" VerticalAlignment="Bottom" Grid.Column="1">:</Label>
								<Label Grid.Row="3" VerticalAlignment="Bottom" Grid.Column="1">:</Label>
								<Label Grid.Row="4" VerticalAlignment="Bottom" Grid.Column="1">:</Label>
								<Label Grid.Row="5" VerticalAlignment="Bottom" Grid.Column="1">:</Label>

								<TextBox IsEnabled="False" Margin="0,0,10,0"  Name="txtIRCMemberNo" Grid.Row="0" Grid.Column="2" Style="{StaticResource MaterialDesignFloatingHintTextBox}" KeyUp="txtIRCMemberNo_KeyUp" LostFocus="txtIRCMemberNo_LostFocus"></TextBox>
								<StackPanel Grid.Row="2" Grid.Column="2" VerticalAlignment="Bottom" Orientation="Horizontal">
									<RadioButton IsEnabled="False" Name="rbtChariman" GroupName="IRCPosition" Margin="0 0 0 0">Chariman</RadioButton>
									<RadioButton IsEnabled="False" Name="rbtSecretary" GroupName="IRCPosition" Margin="10 0 0 0">Secretary</RadioButton>
									<RadioButton IsEnabled="False" Name="rbtCommitteMember" GroupName="IRCPosition" Margin="10 0 0 0">Committee Member</RadioButton>
								</StackPanel>
								<TextBox IsEnabled="False" Margin="0,0,10,0" Name="txtIRCName" IsReadOnly="True" Grid.Row="1" Grid.Column="2" Style="{StaticResource MaterialDesignFloatingHintTextBox}"></TextBox>
								<TextBox IsEnabled="False" Margin="0,0,10,0" Name="txtIRCBankName" IsReadOnly="True"  Grid.Row="3" Grid.Column="2" Style="{StaticResource MaterialDesignFloatingHintTextBox}"></TextBox>
								<TextBox IsEnabled="False" Margin="0,0,10,0" Name ="txtIRCBankAddress" IsReadOnly="True"  Grid.Row="4" Grid.Column="2" Style="{StaticResource MaterialDesignFloatingHintTextBox}"></TextBox>
								<StackPanel Grid.Row="5" Grid.Column="2" Orientation="Horizontal" VerticalAlignment="Bottom">
									<Label Margin="0 0 0 0" VerticalAlignment="Bottom">Office :</Label>
									<TextBox IsEnabled="False" Name="txtIRCTelephoneNo" IsReadOnly="True"  Width="150" Style="{StaticResource MaterialDesignFloatingHintTextBox}"></TextBox>
									<Label Margin="10 0 0 0" VerticalAlignment="Bottom">Mobile :</Label>
									<TextBox IsEnabled="False" Name="txtIRCMobileNo" IsReadOnly="True"  Width="150" Style="{StaticResource MaterialDesignFloatingHintTextBox}"></TextBox>
									<Label Margin="10 0 0 0" VerticalAlignment="Bottom">Fax :</Label>
									<TextBox IsEnabled="False" Name="txtIRCFax" IsReadOnly="True"  Width="150" Style="{StaticResource MaterialDesignFloatingHintTextBox}"></TextBox>

								</StackPanel>
							</Grid>
							<Grid Grid.Row="2" >
								<StackPanel/>
							</Grid>
							<Grid Grid.Row="3" >
								<StackPanel>
									<Label>Dear Sir,</Label>
									<Label>I, the above named IRC hereby confirm that the following: [tick all the boxes as Confirmation]</Label>
									<StackPanel Margin="50 0 0 0">
										<StackPanel Margin="0,0,0,0"  Orientation="Horizontal">
											<CheckBox IsEnabled="False" Name="cbxNameOfPerson" VerticalAlignment="Bottom">Name of Person applying for BF is</CheckBox>
											<TextBox IsEnabled="False" Margin="10,0,0,0" Name="txtIRCResignMemberName" IsReadOnly="True" Width="300" Style="{StaticResource MaterialDesignFloatingHintTextBox}"></TextBox>
										</StackPanel>
										<StackPanel Margin="0,0,0,0"  Orientation="Horizontal">
											<CheckBox IsEnabled="False" Name="cbxPromotedTo" VerticalAlignment="Bottom">She/he was promoted to</CheckBox>
											<TextBox IsEnabled="False" Margin="10,0,0,0" Name="txtIRCPromotedTo" Width="300" Style="{StaticResource MaterialDesignFloatingHintTextBox}"></TextBox>
											<Label VerticalAlignment="Bottom">grade w.e.f.</Label>
											<DatePicker IsEnabled="False" x:Name="dtpGrade" Background="{x:Null}" FontWeight="Bold" HorizontalAlignment="Left" Style="{StaticResource MaterialDesignFloatingHintDatePicker}"  VerticalAlignment="Bottom" Width="106" FontSize="14" Margin="10,0,0,0" />
										</StackPanel>
										<CheckBox IsEnabled="False" Margin="0 20 0 0" Name="cbxBeforePromotion">She/he was a Messenger/Clerical before promotion [Delete which is not applicable]</CheckBox>
										<CheckBox IsEnabled="False" Margin="0 20 0 0" Name="cbxAttached">Attached Job function/Description (compulsory)</CheckBox>
										<CheckBox IsEnabled="False" Margin="0 20 0 0" Name="cbxHereByConfirm">I hereby confirm that he/she got promoted he/she is no longer doing any clerical job function.</CheckBox>
										<CheckBox IsEnabled="False" Margin="0 20 0 0" Name="cbxFilledBy">The Messeger/clerical position has been filled by</CheckBox>
									</StackPanel>
								</StackPanel>
							</Grid>
							<Grid Grid.Row="4" >
								<StackPanel>
									<Label FontWeight="Bold" FontSize="16">BRANCH COMMITTEE VERIFICATION</Label>
									<StackPanel Margin="20 0 0 0">
										<CheckBox IsEnabled="False" Margin="0 10 0 0" Name="cbxBranchCommitteeVerification1">
											<TextBlock TextWrapping="WrapWithOverflow">
                                                I have verified the above and confirm that the declaration by the IRC is correct. The Messenger/Clerical position has been filled by another Messenger/Clerical And;
											</TextBlock>
										</CheckBox>
										<CheckBox IsEnabled="False" Margin="0 10 0 0" Name="cbxBranchCommitteeVerification2">The promoted member is no longer doing Messenger/Clerical job functions</CheckBox>
									</StackPanel>
									<StackPanel Orientation="Horizontal" Margin="0 30 0 0">
										<Label Width="220" VerticalAlignment="Bottom">Branch Committee [Name in full]</Label>
										<TextBox IsEnabled="False" Name="txtBranchCommitteeName" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Width="350"></TextBox>
									</StackPanel>
									<StackPanel Orientation="Horizontal" Margin="0 10 0 0">
										<Label Width="220" VerticalAlignment="Bottom">Zone</Label>
										<TextBox IsEnabled="False" Name="txtBranchCommitteeZone" Style="{StaticResource MaterialDesignFloatingHintTextBox}" Width="350"></TextBox>
										<DatePicker IsEnabled="False" x:Name="dtpBranchCommitteeDate"  FontWeight="Bold" HorizontalAlignment="Left" materialDesign:HintAssist.Hint="Date" Style="{StaticResource MaterialDesignFloatingHintDatePicker}"  VerticalAlignment="Bottom" Width="110" FontSize="14" Margin="150 0 0 0 "  />
									</StackPanel>

								</StackPanel>
							</Grid>
							<Grid Grid.Row="5" >
								<Button Name="btnNextIRC" Width="100" Margin="0 0 50 0" HorizontalAlignment="Right" Click="btnNextIRC_Click">Next</Button>
							</Grid>
						</Grid>
					</ScrollViewer>

				</TabItem>

				<TabItem Header="Bank Address" Name="tbiBank"  FontSize="14" >
					<Grid HorizontalAlignment="Right" Width="865">
						<Rectangle Fill="White" Stroke="Black" StrokeThickness="0"  />
						<ComboBox x:Name="cmbBankCode" FontWeight="Bold" Background="White" HorizontalAlignment="Left" VerticalAlignment="Top" Width="73" materialDesign:HintAssist.Hint="Code" RenderTransformOrigin="0.5,1"  materialDesign:HintAssist.HintOpacity=".26" Margin="61,34,0,0" IsReadOnly="True"/>
						<ComboBox x:Name="cmbBankName" FontWeight="Bold" Background="White" HorizontalAlignment="Left" Margin="139,34,0,0" VerticalAlignment="Top" Width="241" materialDesign:HintAssist.Hint="Bank Name"  materialDesign:HintAssist.HintOpacity=".26" IsReadOnly="True"/>
						<ComboBox x:Name="cmbBranchCode" FontWeight="Bold" Background="White" HorizontalAlignment="Left" Margin="478,34,0,0" VerticalAlignment="Top" Width="81" materialDesign:HintAssist.Hint="Code"  materialDesign:HintAssist.HintOpacity=".26" IsReadOnly="True" />
						<ComboBox x:Name="cmbBranchName" FontWeight="Bold" Background="White"  HorizontalAlignment="Left" Margin="564,34,0,0" VerticalAlignment="Top" Width="246" materialDesign:HintAssist.Hint="Branch Name"  materialDesign:HintAssist.HintOpacity=".26" IsReadOnly="True" />
						<TextBlock x:Name="lblBankName" FontWeight="Bold" HorizontalAlignment="Left" Margin="15,40,0,0" TextWrapping="Wrap" Text="Bank " VerticalAlignment="Top" Width="48" RenderTransformOrigin="0.5,1"/>
						<TextBlock x:Name="lblBranch" FontWeight="Bold" HorizontalAlignment="Left" Margin="428,40,0,0" TextWrapping="Wrap" Text="Branch" VerticalAlignment="Top" Width="51"/>
						<TextBox x:Name="txtAddress" FontWeight="Bold" HorizontalAlignment="Left" Height="50" Margin="15,70,0,0" TextWrapping="Wrap" materialDesign:HintAssist.Hint="Address" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalAlignment="Top" Width="260" IsReadOnly="True"/>
						<TextBox x:Name="txtAddress2" FontWeight="Bold" HorizontalAlignment="Left" Height="50" Margin="15,126,0,0" TextWrapping="Wrap" materialDesign:HintAssist.Hint="Address2" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalAlignment="Top" Width="260" IsReadOnly="True"/>
						<TextBox x:Name="txtAddress3" FontWeight="Bold" HorizontalAlignment="Left" Height="50" Margin="15,180,0,0" TextWrapping="Wrap" materialDesign:HintAssist.Hint="Address3" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalAlignment="Top" Width="260" IsReadOnly="True"/>
						<TextBox x:Name="txtCity" FontWeight="Bold" HorizontalAlignment="Left" Height="50" Margin="312,70,0,0" TextWrapping="Wrap" materialDesign:HintAssist.Hint="City" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalAlignment="Top" Width="260" IsReadOnly="True"/>
						<TextBox x:Name="txtState" FontWeight="Bold" HorizontalAlignment="Left" Height="50" Margin="312,126,0,0" TextWrapping="Wrap" materialDesign:HintAssist.Hint="State" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalAlignment="Top" Width="260" IsReadOnly="True"/>
						<TextBox x:Name="txtPostalCode" FontWeight="Bold" HorizontalAlignment="Left" Height="50" Margin="605,70,0,0" TextWrapping="Wrap" materialDesign:HintAssist.Hint="Postal Code" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalAlignment="Top" Width="250" IsReadOnly="True"/>
						<TextBox x:Name="txtCountry" FontWeight="Bold" HorizontalAlignment="Left" Height="50" Margin="605,126,0,0" TextWrapping="Wrap" materialDesign:HintAssist.Hint="Country" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalAlignment="Top" Width="250" IsReadOnly="True"/>
						<materialDesign:PackIcon Kind="Phone" Foreground="{Binding ElementName=txtPhoneNo, Path=BorderBrush}" Margin="312,199,0,0" Height="31" Width="31"/>
						<TextBox x:Name="txtPhoneNo" FontWeight="Bold" HorizontalAlignment="Left" Height="50" Margin="348,180,0,0" TextWrapping="Wrap" materialDesign:HintAssist.Hint="Phone No" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalAlignment="Top" Width="224" IsReadOnly="True"/>
						<materialDesign:PackIcon Kind="Phone" Foreground="{Binding BorderBrush, ElementName=txtMobileNo}" Margin="605,199,0,0" Height="31" Width="31"/>
						<TextBox x:Name="txtMobileNo" FontWeight="Bold" HorizontalAlignment="Left" Height="50" Margin="640,180,0,0" TextWrapping="Wrap" materialDesign:HintAssist.Hint="Mobile No" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalAlignment="Top" Width="215" IsReadOnly="True" />
						<TextBox x:Name="txtEmail" FontWeight="Bold" HorizontalAlignment="Left" Height="50" Margin="50,236,0,0" TextWrapping="Wrap" materialDesign:HintAssist.Hint="Email" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalAlignment="Top" Width="225" IsReadOnly="True"/>
						<materialDesign:PackIcon Kind="Email" Foreground="{Binding ElementName=txtEmail, Path=BorderBrush}" Margin="14,244,0,0" Height="31" Width="28" RenderTransformOrigin="0.571,3.097"/>
						<TextBlock x:Name="lblBankName_Copy1" FontWeight="Bold" HorizontalAlignment="Left" Margin="312,260,0,0" TextWrapping="Wrap" Text="Date of Emp." VerticalAlignment="Top" Width="88"/>
						<DatePicker x:Name="dtpDOEmp" FontWeight="Bold" HorizontalAlignment="Left" materialDesign:HintAssist.Hint="Date Of Emp" Style="{StaticResource MaterialDesignFloatingHintDatePicker}" Margin="415,254,0,0" VerticalAlignment="Top" Width="157" FontSize="14" Height="32" />
						<TextBox x:Name="txtSalary" FontWeight="Bold" HorizontalAlignment="Left" Height="50" Margin="605,236,0,0" TextWrapping="Wrap" materialDesign:HintAssist.Hint="Salary" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalAlignment="Top" Width="175" IsReadOnly="True" />
						<TextBlock x:Name="lblBankName_Copy3" FontWeight="Bold" HorizontalAlignment="Left" Margin="15,317,0,0" TextWrapping="Wrap" Text="Levy" VerticalAlignment="Top" Width="39" RenderTransformOrigin="0.812,-0.474"/>
						<ComboBox x:Name="cmbLevy" FontWeight="Bold" Background="White" HorizontalAlignment="Left" Margin="59,313,0,0" VerticalAlignment="Top" Width="106" materialDesign:HintAssist.Hint="Levy"  materialDesign:HintAssist.HintOpacity=".26" IsReadOnly="True">
							<ComboBoxItem>N/A</ComboBoxItem>
							<ComboBoxItem>Yes</ComboBoxItem>
							<ComboBoxItem>No</ComboBoxItem>
						</ComboBox>
						<ComboBox x:Name="cmbTDF" FontWeight="Bold" Background="White" HorizontalAlignment="Left" Margin="348,315,0,0" VerticalAlignment="Top" Width="108" materialDesign:HintAssist.Hint="TDF"  materialDesign:HintAssist.HintOpacity=".26" IsReadOnly="True">
							<ComboBoxItem Content="N/"/> a
							<ComboBoxItem Content="Yes"/>
							<ComboBoxItem Content="No"/>
						</ComboBox>
						<TextBlock x:Name="lblTDF" FontWeight="Bold" HorizontalAlignment="Left" Margin="312,317,0,0" TextWrapping="Wrap" Text="TDF" VerticalAlignment="Top" Width="31"/>
						<TextBox x:Name="txtLevyAmount" FontWeight="Bold" HorizontalAlignment="Left" Height="50" Margin="185,292,0,0" TextWrapping="Wrap" materialDesign:HintAssist.Hint="Levy Amount" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalAlignment="Top" Width="90" IsReadOnly="True"/>
						<TextBox x:Name="txtTDFAmount" FontWeight="Bold" HorizontalAlignment="Left" Height="50" Margin="478,292,0,0" TextWrapping="Wrap" materialDesign:HintAssist.Hint="TDF Amount" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalAlignment="Top" Width="94" IsReadOnly="True"/>
						<Button Name="btnNextBank" Width="100" Margin="0 0 50 50" HorizontalAlignment="Right" VerticalAlignment="Bottom" Click="btnNextBank_Click">Next</Button>

					</Grid>
				</TabItem>

				<TabItem Header="Residential Address" Name="tbiResidentialAddress" FontSize="14">
					<Grid>
						<Rectangle Fill="White" Stroke="Black" StrokeThickness="0"  />
						<TextBox x:Name="txtResAddress1" FontWeight="Bold" HorizontalAlignment="Left" Height="50" Margin="72,35,0,0" TextWrapping="Wrap" materialDesign:HintAssist.Hint="Address" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalAlignment="Top" Width="324" IsReadOnly="True"/>
						<TextBox x:Name="txtResAddress2" FontWeight="Bold" HorizontalAlignment="Left" Height="50" Margin="72,91,0,0" TextWrapping="Wrap" materialDesign:HintAssist.Hint="Address 2" Style="{StaticResource MaterialDesignFloatingHintTextBox}" VerticalAlignment="Top" Width="324" IsReadOnly="True"/>
