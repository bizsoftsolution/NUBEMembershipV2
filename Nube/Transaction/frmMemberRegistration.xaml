                <TextBox x:Name="txtHeading" FontWeight="Bold" TextAlignment="Center" BorderThickness="0" HorizontalAlignment="Left" Height="41" TextWrapping="Wrap" Text="MEMBER REGISTRATION" VerticalAlignment="Top" FontSize="18" Margin="540,0,0,0" />
            </materialDesign:Card>
        </Grid>
        <Grid Grid.RowSpan ="1" Grid.Column="0">
